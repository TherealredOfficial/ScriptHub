    function AutoBicep()
        local onClick = game:GetService("ReplicatedStorage"):WaitForChild("Packages").Knit.Services.ToolService.RE.onClick

        while AutoTrainBicepEnabled do
            pcall(onClick.FireServer, onClick)
            wait(0.000000001)
        end
    end
   

    function AutoHands()
        local onClick = game:GetService("ReplicatedStorage"):WaitForChild("Packages").Knit.Services.ToolService.RE.onClick

        while AutoTrainHandsEnabled do
            pcall(onClick.FireServer, onClick)
            wait(0.00000001)
        end
    end
    
    
